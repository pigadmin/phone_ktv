<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <!--图标显示类型-->
        <attr name="src_log_type" format="reference" />
        <!--标题显示类型-->
        <attr name="title_type" format="string" />
        <!--多少首是否隐藏-->
        <attr name="num_first_isState" format="boolean" />
        <!--多少首动态设置文字-->
        <attr name="num_first_string" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomEditView">
        <!--图标显示类型-->
        <attr name="src_log_type1" format="reference" />
        <!--标题显示类型-->
        <attr name="title_hint_type" format="string" />
        <!--验证码是否隐藏-->
        <attr name="verd_code_isState" format="boolean" />
        <!--输入字符设定-->
        <attr name="input_digits" format="string" />
        <!--输入法设定-->
        <attr name="input_type" format="integer" />
        <!--输入长度-->
        <attr name="maxLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomTopTitleView">
        <!--标题显示类型-->
        <attr name="back_text_type" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomSetupView">
        <!--标题显示类型-->
        <attr name="setup_text" format="string" />
        <!--是否显示"不开启"文字-->
        <attr name="no_open_isState" format="boolean" />
        <!--是否显示SwitchButton开关-->
        <attr name="no_switch_btn_isState" format="boolean" />
        <!--SwitchButton开关状态-->
        <attr name="switch_btn_isState" format="boolean" />
    </declare-styleable>

</resources>